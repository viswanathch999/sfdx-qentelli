    <validationRules>
        <fullName>YNG_Child_Age_Validation</fullName>
        <active>true</active>
        <description>Validation rule to prohibit the addition of Opportunity Products with the name containing &quot;YNG&quot; to Opportunities where there is not a Child with Product Eligibility set to &quot;YNG&quot;.</description>
        <errorConditionFormula>AND(
 Contact__c !=null,
OR(  
Contact__r.Product_Eligibility__c &lt;&gt;&quot;YNG&quot;)
,
  ISPICKVAL(Product2.Exclusive__c, &quot;YNG&quot;) 
)</errorConditionFormula>
        <errorMessage>You cannot associate a YNG product to an opportunity where the contact is not between the ages of 18 and 30</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>quantity_of_each_item_should_be_one</fullName>
        <active>true</active>
        <description>Quantity &gt; 1; quantity for each benefit item to be one</description>
        <errorConditionFormula>Quantity &gt;1</errorConditionFormula>
        <errorMessage>Quantity Cannot be more than One</errorMessage>
    </validationRules>
