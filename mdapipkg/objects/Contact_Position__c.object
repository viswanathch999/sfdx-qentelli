    <validationRules>
        <fullName>Account_or_Event</fullName>
        <active>true</active>
        <errorConditionFormula>AND( 
(ISBLANK(Account__c)) , 
(ISBLANK(Event__c)))</errorConditionFormula>
        <errorMessage>Please select either an Account or an Event</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_RT_nd_Position_Member_Type_match</fullName>
        <active>true</active>
        <description>Member of specific record type can be attached to Contact Position having position of particular Member Type only.
Contact of type Non-Member cannot be attached to any Contact Position.</description>
        <errorConditionFormula>AND(NOT(ISBLANK( Contact__c)) , 
OR(NOT(
OR(
AND( Contact__r.Record_Type_Name__c = &apos;Spouse/Partner&apos;,OR(IsPICKVAL(Position__r.Member_Type__c, &apos;Either a Member or Spouse/Partner&apos;),IsPICKVAL(Position__r.Member_Type__c, &apos;Spouse/Partner&apos;))),
AND( Contact__r.Record_Type_Name__c = &apos;Member&apos;,OR(IsPICKVAL(Position__r.Member_Type__c, &apos;Either a Member or Spouse/Partner&apos;),IsPICKVAL(Position__r.Member_Type__c, &apos;Member&apos;))),
AND( Contact__r.Record_Type_Name__c = &apos;Team Member&apos;,IsPICKVAL(Position__r.Member_Type__c, &apos;Management Team Member&apos;)),
AND( Contact__r.Record_Type_Name__c = &apos;Chapter Administrator&apos;,OR(IsPICKVAL(Position__r.Member_Type__c, &apos;Chapter Administrator&apos;),IsPICKVAL(Position__r.Member_Type__c, &apos;Management Team Member&apos;)))
)),
Contact__r.Record_Type_Name__c = &apos;Non-Member&apos;
))</errorConditionFormula>
        <errorMessage>The Contact Record Type and the Member type on Position must be the same</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contact_is_required_for_filled_positions</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
ISPICKVAL( Identification_Status__c , &apos;Filled&apos;),
ISBLANK( Contact__c )
)</errorConditionFormula>
        <errorMessage>Please indicate which Contact will be filling this position.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_Contact_Position_Fields</fullName>
        <active>true</active>
        <description>I need to be required to enter certain fields when assigning an officer position: 
Status, Member Type, Start Date, End Date and either Account or Event are required. 
I should not be able to save a record if all the required fields aren&apos;t entered.</description>
        <errorConditionFormula>OR(
ISBLANK(TEXT(Status__c)),
ISBLANK(Start_Date__c),
ISBLANK(End_Date__c), 
AND(
ISBLANK(Account__c),
ISBLANK(Event__c)
)
)</errorConditionFormula>
        <errorMessage>Status, Start Date, End Date and either Account or Event are required.</errorMessage>
    </validationRules>
